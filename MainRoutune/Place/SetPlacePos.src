&ACCESS RVO1
&PARAM EDITMASK = *
DEF SetPlacePos()
   ;Esses valores devem vir sempre da main
   
   ;XPlace     = faz o place no devido local
   XApp1Place = MatrizPontos[ReceitaAtivo,PlaceAtivo].XApp1Place
   XApp2Place = MatrizPontos[ReceitaAtivo,PlaceAtivo].XApp2Place
   XPlace     = MatrizPontos[ReceitaAtivo,PlaceAtivo].XPlace  
   
   ;CamadaAtiva
   XApp1Place.Z = XApp1Place.Z + (CamadaAtiva-1)*AlturaCaixa
   XApp2Place.Z = XApp2Place.Z + (CamadaAtiva-1)*AlturaCaixa
   XPlace.Z=XPlace.Z + (CamadaAtiva-1)*AlturaCaixa
END